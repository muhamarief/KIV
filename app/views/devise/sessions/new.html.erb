

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="login-block login">
  <h1>Login</h1>
  
    <%= f.email_field :email, autofocus: true, placeholder:"Email", id:"username" %>

    <%= f.password_field :password, autocomplete: "off", placeholder:"Password", id:"password" %>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.label :remember_me %><%= f.check_box :remember_me%>
    </div>
  <% end %>
    <%= f.submit "Log in          ", id:"button"%>
  </div>
<% end %>

<div class="login-block login">
  <%= button_to "Facebook Login", user_facebook_omniauth_authorize_path, class:"btn btn-primary"%>
</div>


