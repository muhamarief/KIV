<!-- partial f for new.html.erb and edit.html.erb -->
<%= form_for @cinema do |f| %>

<p>
  <%= f.label :cinema_name %><br>
  <%= f.text_field :cinema_name %>
</p>

<p>
  <%= f.label :contact %><br>
  <%= f.text_field :contact %>
</p>

<p>
  <%= f.label :address %><br>
  <%= f.text_area :address %>
</p>

<p>
  <%= f.label :state %><br>
  <%= f.text_field :state %>
</p>

<p>
  <%= f.label :company %><br>
  <%= f.text_field :company %>
</p>

<p>
  <%= f.label :longitude %><br>
  <%= f.text_field :longitude %>
</p>

<p>
  <%= f.label :latitude %><br>
  <%= f.text_field :latitude %>
</p>

<%= f.file_field :photos, multiple: true %>


<p>
  <%= f.submit "submit cinema info"%>
</p>

<%end%>